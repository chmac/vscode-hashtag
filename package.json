{
  "name": "vscode-hashtag",
  "displayName": "vscode-hashtag",
  "description": "Insert #hashtags in VS Code",
  "version": "0.0.1",
  "publisher": "chmac",
  "repository": "https://github.com/chmac/vscode-hashtag",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.insertHashtag"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.insertHashtag",
        "title": "Insert a random #hashtag"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "tslint -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^10.14.17",
    "@types/vscode": "^1.34.0",
    "tslint": "^5.16.0",
    "typescript": "^3.5.1"
  }
}
